    <sys_script_client action="DELETE">
        <active>true</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description>When the unit changes calculate the unit value</description>
        <field>unit</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Changes Current Unit</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if (isLoading || newValue === '') {
      return;
   }	
	
	var newTemp = null; 
	var temp = null;
	var unit = g_form.getValue('unit'); 
	
	function CtoF () {
		temp = g_form.getValue('temp');	
		newTemp = (temp * 1.8) +32 ; 
	}
	
	function FtoC () {
		temp = g_form.getValue('temp');	
		newTemp = (temp -32) * .55556 ; 

	}
	
	if (unit == "Farenheit") {	
	FtoC();
	}
	else {
		CtoF();
	}
	g_form.setValue('temp', newTemp);

}]]></script>
